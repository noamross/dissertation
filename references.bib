  issn		= {1742-5662},
  journal	= {Journal of the Royal Society, Interface / the Royal
		  Society},
  keywords	= {Algorithms,Animals,Disease Outbreaks,Disease Outbreaks:
		  prevention \& control,Humans,Infection Control,Infection
		  Control: methods,Models,Plant Diseases,Plant Diseases:
		  prevention \& control,Public
		  Health,Quarantine,Statistical,Theoretical,Treatment Outcome},
  month		= dec,
  number	= {41},
  pages		= {1135--44},
  pmid		= {19324686},
  title		= {{Optimal control of epidemics in metapopulations.}},
